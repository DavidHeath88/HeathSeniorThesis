        "nfl_data_2018 = {v: k for k, v in nfl_data_2018_reversed.items()}\n",
        "\n",
        "\n",
        "def query_gpt(pick):\n",
        "    prompt = f\"Who was the {pick} pick in the 2018 NFL Draft?\"\n",
        "\n",
        "    response = openai.ChatCompletion.create(\n",
        "        model=\"model\",\n",
        "        messages=[\n",
        "            {\"role\": \"user\", \"content\": prompt}\n",
        "        ],\n",
        "        max_tokens=50\n",
        "    )\n",
        "\n",
        "    return response['choices'][0]['message']['content'].strip()\n",
        "\n",
        "\n",
        "def is_name_in_response(response, actual_name):\n",
        "    actual_name_pattern = re.compile(re.escape(actual_name), re.IGNORECASE)\n",
        "    return bool(actual_name_pattern.search(response))\n",
        "\n",
        "def color_code(val):\n",
        "    color = 'green' if val == 'Correct' else 'red'\n",
        "    return f'background-color: {color}'\n",
        "\n",
        "results = []\n",
        "pick_range = range(1, 256)\n",
        "\n",
        "for pick in pick_range:\n",
        "    try:\n",
        "        actual_answer = nfl_data_2018_reversed.get(pick, \"Unknown\")\n",
        "        response = query_gpt(pick)\n",
        "\n",
        "        correctness = \"Correct\" if is_name_in_response(response, actual_answer) else \"Incorrect\"\n",
        "\n",
        "        results.append({\n",
        "            'Pick': pick,\n",
        "            'Response': response,\n",
        "            'Actual': actual_answer,\n",
        "            'Correctness': correctness\n",
        "        })\n",
        "    except Exception as e:\n",
        "        print(f\"Error querying GPT for pick {pick}: {e}\")\n",
        "\n",
        "df_results = pd.DataFrame(results)\n",
        "csv_file_path = '/content/drive/MyDrive/nfl_draft_gpt4_results-testrun#5.csv'\n",
        "df_results.to_csv(csv_file_path, index=False)\n",
        "\n",
        "print(f\"Results saved to {csv_file_path}\")"
